  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "rollup -c",
    "prepare": "npm run build",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "cross-fetch": "^4.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.19.25",
    "jest": "^29.7.0",
    "openapi-typescript": "^6.7.0",
    "rollup": "^4.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "datavist",
    "sdk",
    "data",
    "extraction",
